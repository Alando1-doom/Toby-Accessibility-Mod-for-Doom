ACTOR ProxyChecker
{
    	Health 10000000000000000000000
    	Radius 8
    	Height 8
    	Speed 0
	    Mass 2000000000000
    	PainChance 0
        DONTHURTSHOOTER
    	MONSTER 
        +JUSTHIT
        +DONTGIB
    	+FLOORCLIP
        +NOTARGET
        -COUNTKILL
        -SHOOTABLE
        +NOBLOOD
        +FASTER
        +ALWAYSFAST
		+NOCLIP
		+INVULNERABLE
		+GHOST
		+LOOKALLAROUND
    	PainSound "null"
        DeathSound "null"
    	MeleeDamage 0
    	States
    	{
	Spawn: 
	    TNT1 A 0 A_JumpIfInventory("MyExistence", 2, "Death")
		TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
        TNT1 A 1 A_Look 
        TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
	    TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
		TNT1 A 1 A_GiveInventory("MyExistence", 1)
        Loop 
    See: 
	    TNT1 A 0 A_JumpIfInventory("MyExistence", 2, "Death")
        TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
	    TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
		TNT1 A 1 A_GiveInventory("MyExistence", 1)
        TNT1 A 1 A_Chase
        Loop
    Missile:
	    TNT1 A 0 A_JumpIfInventory("MyExistence", 2, "Death")
		TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
        TNT1 A 1 A_FaceTarget
        TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
	    TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
		TNT1 A 1 A_GiveInventory("MyExistence", 1)
        Goto See
    Melee:
	    TNT1 A 0 A_JumpIfInventory("MyExistence", 2, "Death")
        TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
	    TNT1 A 0 A_JumpIfCloser(128, "BeaconWarm")
		TNT1 A 1 A_GiveInventory("MyExistence", 1)
        TNT1 A 0 A_FaceTarget
        Goto See
    Pain: 
        TNT1 A 1
        TNT1 A 1 A_Pain 
        Goto See
    BeaconWarm:
	     TNT1 A 0 A_JumpIfInventory("MyExistence", 2, "Death")
         TNT1 A 1 A_PlaySoundEx("prox/warm", "SoundSlot5", 0, 0)
		 TNT1 A 0 A_JumpIfCloser(16, "Death")
		 TNT1 A 19 A_JumpIfCloser(16, "Death")
		 TNT1 A 1 A_GiveInventory("MyExistence", 1)
         Goto See
	BeaconWarmer:
	     TNT1 A 0 A_JumpIfInventory("MyExistence", 2, "Death")
         TNT1 A 1 A_PlaySoundEx("prox/warmer", "SoundSlot5", 0, 0)
		 TNT1 A 0 A_JumpIfCloser(16, "Death")
		 TNT1 A 19 A_JumpIfCloser(16, "Death")
		 TNT1 A 1 A_GiveInventory("MyExistence", 1)
         Goto See
	BeaconHot:
	     TNT1 A 0 A_JumpIfInventory("MyExistence", 2, "Death")
         TNT1 A 1 A_PlaySoundEx("prox/warmer", "SoundSlot5", 0, 0)
		 TNT1 A 0 A_JumpIfCloser(16, "Death")
		 TNT1 A 19 A_JumpIfCloser(16, "Death")
		 TNT1 A 1 A_GiveInventory("MyExistence", 1)
         Goto See
    Death: 
        TNT1 A 1
        Stop
        }
}

//***************************************************************
// Checker Counter
//***************************************************************

actor MyExistence : Inventory
{
  inventory.pickupmessage " "
  inventory.icon "TNT1A0"
inventory.amount 2
inventory.maxamount 2
 inventory.InterHubAmount 0
  States
      {
       Spawn:
                  TNT1 A 6
                  loop
       }
}